; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Vulnus"
config/description="Vulnus"
run/main_scene="res://scenes/Startup.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Vulnus"
boot_splash/image="res://assets/images/beatgamedev.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0.0470588, 0.0627451, 0.0941176, 1 )
config/icon="res://assets/images/vulnus.png"
config/windows_native_icon="res://assets/images/vulnus.ico"

[autoload]

Global="*res://scripts/Global.cs"

[display]

window/size/width=1280
window/size/height=720
window/size/test_width=1024
window/size/test_height=576
window/vsync/use_vsync=false

[gui]

timers/tooltip_delay_sec=0.3

[physics]

common/enable_pause_aware_picking=true
3d/default_gravity_vector=Vector3( 0, 0, 0 )

[rendering]

quality/filters/sharpen_intensity=0.4
quality/driver/fallback_to_gles2=true
threads/thread_model=2
quality/filters/use_nearest_mipmap_filter=true
quality/filters/msaa=1
quality/filters/use_fxaa=true
